{
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "index.html",
    "page_*.html"
  ],
  "watch": [
    ".",
    "css",
    "js",
    "layouts",
    "sections"
  ],
  "execMap": {
    "js": "node --harmony"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json html scss"
}
